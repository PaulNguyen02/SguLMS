<div class="col-md-12">
    <div class="white_shd full margin_bottom_30">
        <div class="full graph_head">
            <div class="heading1 margin_0">
                <h2>Học kỳ {{semester.HOCKY}}-{{semester.NIENKHOA}}</h2>
            </div>
        </div>
        <div class="table_section padding_infor_info">
            <div class="table-responsive-sm">
                <table class="table">
                    <thead class="table-primary">
                        <tr>
                            <th>Mã Môn Học</th>
                            <th>Nhóm/tổ</th>
                            <th>Tên Môn Học</th>
                            <th>Số tín chỉ</th>
                            <th>Điểm quá trình (10)</th>
                            <th>Điểm cuối kỳ (10)</th>
                            <th>Điểm tổng kết (10)</th>
                            <th>Điểm tổng kết (4)</th>
                            <th>Xếp Loại</th>
                            <th>Kết Quả</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each grade_list}}
                            <tr>
                                <td>{{this.MAMH}}</td>
                                <td>{{this.NHOM}}</td>
                                <td>{{this.TENMH}}</td>
                                <td>{{this.STC}}</td>
                                <td>{{this.DIEMQUATRINH10}}</td>
                                <td>{{this.DIEMCUOIKY10}}</td>
                                <td>{{this.DIEMTONGKET10}}</td>
                                <td>{{this.DIEMTONGKET4}}</td>
                                <td>{{this.XEPLOAI}}</td>
                                <td>{{this.KETQUA}}</td>
                            </tr>
                        {{/each}}
                    </tbody>
                </table>
                <form>
                    <div class="read_more">
                        <div class="center"><a class="main_bt read_bt" href="#">In bảng điểm</a></div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
